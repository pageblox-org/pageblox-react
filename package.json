{
  "name": "pageblox-react",
  "version": "1.3.5",
  "description": "React library to review UX/UI code changes in the browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/pageblox-org/pageblox-react.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs && npm run build:styles",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:styles": "postcss src/index.css -o dist/index.css",
    "rollup": "rollup -c"
  },
  "author": "Pageblox",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "date-fns": "^2.29.3",
    "firebase": "^9.15.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-uuid": "^2.0.0",
    "rollup": "^3.9.1",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.4"
  }
}
